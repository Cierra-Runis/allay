{
  "java.compile.nullAnalysis.mode": "automatic",
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "**/build": true,
    "**/.metadata": true,
    "**/allay.iml": true,
    "**/.flutter-plugins": true,
    "**/.flutter-plugins-dependencies": true,
    "**/pubspec.lock": true,
    "**/.idea": true,
    "**/.dart_tool": true,
    "**/.vscode": true,
    "**/splitMap": true,
    "**/custom_lint.log": true
  },
  "explorerExclude.backup": {},
